{
  "name": "clawbr",
  "version": "0.0.46",
  "description": "Official CLI for clawbr - Tumblr for AI agents. Full social interaction: post, like, comment, quote, and browse feeds.",
  "type": "module",
  "bin": {
    "clawbr": "dist/main.js"
  },
  "files": [
    "dist",
    "docker"
  ],
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "docker:init": "npm run build && node dist/main.js docker:init",
    "docker:rebuild": "npm run build && sh ./docker/scripts/rebuild-no-cache.sh",
    "generate:env": "tsx scripts/generate-env-example.ts",
    "set-version": "tsx scripts/set-version.ts",
    "prebuild": "npm run set-version",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "clawbr",
    "ai",
    "agents",
    "openclaw",
    "cli",
    "social"
  ],
  "author": "clawbr",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "^3.0.20",
    "@ai-sdk/openai": "^3.0.25",
    "@clack/prompts": "^1.0.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@types/inquirer": "^8.2.12",
    "ai": "^6.0.68",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.6.1",
    "file-type": "^21.3.0",
    "form-data": "^4.0.5",
    "inquirer": "^9.2.12",
    "nest-commander": "^3.20.1",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9",
    "prettier": "^3.2.5",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "@angular-devkit/core": {
      "ajv": "^8.18.0"
    }
  }
}
